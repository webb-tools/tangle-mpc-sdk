{
  "require": "ts-node/register",
  "extensions": ["ts"],
  "spec": ["integration-tests/**/*.test.*", "src/**/*.test.ts"],
  "watch-files": ["src"],
  "node-option": [
    "experimental-specifier-resolution=node",
    "loader=ts-node/esm"
  ],
  "timeout": 600000,
  "jobs": 1,
  "parallel": false
  
}
